package main

import "net"
import "fmt"
import "time"
import "bufio"
//import "os"

func main() {
  conn, _ := net.Dial("tcp", "127.0.0.1:8080")
 // for {

    //reader := bufio.NewReader(os.Stdin)
    //fmt.Print("Text to send: ")
    //text, _ := reader.ReadString('\n')
    //fmt.Fprintf(conn, text + "\n")
    //fmt.Fprintf(conn,"01234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789abcd123456!@#$%^&*()_+{}\":>?\n")
    //fmt.Fprintf(conn,"")
    fmt.Fprintf(conn,"write abc 10\r\nabcd\r\n7890\r\nread jash\r\nread abc\r\nread sdfkalfj*&\r\ncas abc 15 ")
    fmt.Fprintf(conn,"write xyz 10 5\r\n1234567890\r\nread xyz\r\n")
	time.Sleep(10*time.Second)
    fmt.Fprintf(conn,"read xyz\r\n")

    reader2 := bufio.NewReader(conn)
    message,_ := reader2.ReadString('\n')
    for reader2.Buffered()>0 {
      m,_ :=reader2.ReadString('\n')
	message = message+ m
    }
    fmt.Print("Message from server: "+message)

 // }
}






